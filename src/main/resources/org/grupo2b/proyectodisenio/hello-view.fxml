<?import org.grupo2b.proyectodisenio.logica.interfaz.Boton?>
<?import org.grupo2b.proyectodisenio.logica.interfaz.Knob?>

<VBox alignment="CENTER" prefHeight="103.0" prefWidth="176.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.grupo2b.proyectodisenio.logica.interfaz.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" text="Hello!" />
    <Knob />
    <Boton fx:id="boton" maxHeight="-Infinity" maxWidth="-Infinity" VBox.vgrow="NEVER" />
</VBox>
